#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the height of the triangle"<<endl;
    cin>>n;
    for(int i = 1;i<=n;i++)
    {
        for(int j = 1;j<=i;j++) //when i=1 j is also equal to one
        {
            cout<<"*";
        }
        cout<<endl;
    }
    return 0;
}
